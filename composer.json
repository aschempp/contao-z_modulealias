{
    "name": "terminal42/contao-z_modulealias",
    "description":"Module Alias extension for Contao Open Source CMS",
    "keywords":["contao", "frontend", "module"],
    "type":"contao-module",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"terminal42 gmbh",
            "homepage":"http://terminal42.ch"
        }
    ],
    "require":{
        "php":">=5.3.2",
        "contao/core":"~3.2",
        "contao-community-alliance/composer-installer":"*"
    },
    "replace": {
        "contao-legacy/z_modulealias": "self.version"
    },
    "extra":{
        "contao": {
            "sources":{
                "": "system/modules/z_modulealias"
            }
        }
    }
}
